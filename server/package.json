{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod:forever",
    "server": "babel-node ./src/bin/www",
    "locdev": "pm2 start npm -- run server",
    "local": "NODE_ENV=development pm2 start -i 0 --watch --ignore-watch='node_modules' ./src/bin/www.js --name server --interpreter ./node_modules/@babel/cli/bin/babel.js",
    "local:prod": "NODE_ENV=production npm-run-all clean build && pm2 start -i 0 ./dist/bin/www.js --name server",
    "server:prod": "node ./dist/bin/www",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "dev": "NODE_ENV=development npm-run-all server",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "deploy": "NODE_ENV=production npm-run-all clean build",
    "watch": "nodemon",
    "prod:cluster": "NODE_ENV=production npm-run-all clean build cluster",
    "dev:cluster": "NODE_ENV=development npm-run-all cluster:dev",
    "cluster": "node ./dist/cluster",
    "cluster:dev": "babel-node ./src/cluster",
    "stress": "artillery run stress-test.yml",
    "test": "snyk test",
    "start:forever": "node start.js",
    "stop:forever": "forever stopall",
    "prod:forever": "NODE_ENV=production npm-run-all stop:forever clean build start:forever",
    "dev:forever": "NODE_ENV=development npm-run-all stop:forever clean build start:forever"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.256.0",
    "@babel/cli": "^7.12.0",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-flow": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@sendgrid/mail": "^6.5.5",
    "@sentry/node": "^5.11.2",
    "accesscontrol": "^2.2.1",
    "aws-sdk": "^2.814.0",
    "axios": "^0.21.3",
    "bcryptjs": "^2.4.3",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "ejs": "^3.1.7",
    "email-templates": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.2",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.1.10",
    "express-mongo-sanitize": "^2.0.0",
    "express-winston": "^4.0.5",
    "firebase-admin": "^11.4.1",
    "forever": "^4.0.0",
    "form-data": "^3.0.0",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "http-errors": "~1.6.2",
    "http-status": "^1.3.2",
    "joi": "^17.3.0",
    "json2csv": "^4.5.4",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^3.5.7",
    "mongoose": "^5.13.15",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.9.0",
    "node-cleanup": "^2.1.2",
    "nodemon": "^2.0.17",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.1",
    "redis": "^3.1.1",
    "rimraf": "^2.7.1",
    "swagger-jsdoc": "^3.7.0",
    "swagger-ui-express": "^4.2.0",
    "uuid": "^3.4.0",
    "winston": "^3.2.1",
    "winston-cloudwatch": "^3.1.0",
    "winston-mongodb": "^5.0.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "flow-bin": "^0.117.0",
    "snyk": "^1.361.3"
  }
}
